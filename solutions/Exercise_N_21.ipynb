   "source": [
    "def string_match(a, b):\n",
    "    result = 0\n",
    "    for i in range(min([len(a), len(b)])-1):\n",
    "        if a[i] == b[i] and a[i+1] == b[i+1]:\n",
    "            result = result+ 1\n",
    "    return result"
   ]
  },
